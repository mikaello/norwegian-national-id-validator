  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/node": "22.18.1",
    "jest": "30.0.5",
    "mockdate": "3.0.5",
    "timezone-mock": "1.3.6",
    "ts-jest": "29.4.1",
    "typedoc": "0.28.10",
    "typedoc-plugin-markdown": "4.8.1",
